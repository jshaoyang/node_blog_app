{
	"appenders":[
		{
			"type":"console",
			"category":"console"
		},
		{
			"type": "dateFile",
			"filename":"./logs/network/listen.log",
			"pattern":"-yyyy-MM",
			"category":"listen",
			"encoding":"utf-8"
		},
		{
			"type": "dateFile",
			"filename":"./logs/network/request.log",
			"pattern":"-yyyy-MM",
			"category":"request",
			"encoding":"utf-8"
		},
		{
			"type": "dateFile",
			"filename":"./logs/network/upload.log",
			"pattern":"-yyyy-MM",
			"category":"upload",
			"encoding":"utf-8"
		}
	],        
	"replaceConsole": true,
	"levels":{
		"listen":"INFO",
		"request":"INFO",
		"upload":"INFO"
	}
}
